# Uncomment the next line to define a global platform for your project
platform :ios, '12.0'

target 'flash (iOS)' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for flash (iOS)
  pod 'RealmSwift', '~>10'
end

target 'flash (macOS)' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for flash (macOS)
  pod 'RealmSwift', '~>10'
end
